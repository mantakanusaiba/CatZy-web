@{
    ViewData["Title"] = "Catzy - User Dashboard";

}

<!-- Navbar -->
<nav class="navbar">
    <div class="logo">Catzy</div>
    <ul class="nav-links">
        <li><a href="#">Home</a></li>
        <li><a href="@Url.Action("Index", "Shop")">Pet Shop</a></li>
        <li><a href="#">Adoption</a></li>
        <li><a href="@Url.Action("Report", "Rescue")">Rescue</a></li>
        <li><a href="#">Vet Appointment</a></li>
        <li><a href="#">Pet Diary</a></li>
        <li><a href="#">About Us</a></li>
        <li><a href="/Account/Logout">Logout</a></li>
    </ul>
</nav>
<!-- Main Section -->
<!-- Main Section -->
<div class="main-section">
    <div class="content">
        <h1>
            Welcome to Catzy, @ViewBag.User
        </h1>
        <p>
            Your one-stop destination for all pet care needs. <br />
            Shop for quality products, adopt a furry friend, or book a vet appointment.
        </p>
        <div class="buttons">
            <a href="@Url.Action("Index", "Shop")" class="btn">Shop Now</a>
            <a href="#" class="btn">Adopt a pet</a>
        </div>
    </div>

    <!-- Image Gallery -->
    <div class="image-gallery">
        <div class="gallery-item">
            <img src="~/Content/image/cat.jpg" alt="Cute cat" />
            <div class="image-overlay">
                <span>Happy Cats</span>
            </div>
        </div>
        <div class="gallery-item">
            <img src="~/Content/image/cat7.jpg" alt="Whiskers" />
            <div class="image-overlay">
                <span>Playful Friends</span>
            </div>
        </div>
        <div class="gallery-item">
            <img src="~/Content/image/cat3.jpg" alt="Larry" />
            <div class="image-overlay">
                <span>Adorable Companions</span>
            </div>
        </div>
    </div>

</div>
<!-- Our Services Section -->
<section class="services-section">
    <h2>Our Services</h2>
    <div class="services-container">
        <div class="services-info">
            <span class="tag">Pet Shop</span>
            <h3>Quality Products for Your Pets</h3>
            <p>
                Discover premium food, toys, accessories, and more for your beloved pets.
                We carefully select products that ensure their health and happiness.
            </p>
            <a href="@Url.Action("Index", "Shop")" class="browse-btn">Browse Products ‚Üí</a>
        </div>
        <div class="services-grid">
            <div class="service-card">
                <img src="~/Content/image/food.png" alt="Pet Food" />
                <p>Pet Food</p>
            </div>
            <div class="service-card">
                <img src="~/Content/image/mouse.png" alt="Toys" />
                <p>Toys</p>
            </div>
            <div class="service-card">
                <img src="~/Content/image/health.png" alt="Health Care" />
                <p>Health Care</p>
            </div>
            <div class="service-card">
                <img src="~/Content/image/acce.png" alt="Accessories" />
                <p>Accessories</p>
            </div>
        </div>
    </div>
</section>
<!-- Vet Appointment Section -->
<section class="vet-appointment-section">
    <div class="vet-appointment-container">
        <!-- Left Column -->
        <div class="vet-info">
            <span class="tag">Vet Appointment</span>
            <h3>Professional Veterinary Care</h3>
            <p>
                Schedule appointments with our experienced veterinarians for check-ups,
                vaccinations, treatments, and consultations. <br />
            </p>
            <a href="#" class="book-btn">Book an Appointment ‚Üí</a>
        </div>
        <!-- Right Column -->
        <div class="vet-services">
            <h3>Available Services</h3>
            <ul>
                <li>Routine Check-ups</li>
                <li>Vaccinations</li>
                <li>Dental Care</li>
                <li>Surgery</li>
                <li>Nutritional Counseling</li>
            </ul>
        </div>
    </div>
</section>
<!-- Adoption Section -->
<section class="adoption-section">
    <div class="adoption-container">
        <!-- Pet Cards -->
        <div class="pet-card">
            <img src="~/Content/image/whisker.jpg" alt="Whiskers" />
            <h4>Whiskers</h4>
            <p>2 years old, tabby</p>
            <p>Playful, friendly</p>
        </div>
        <div class="pet-card">
            <img src="~/Content/image/larry.jpg" alt="Larry" />
            <h4>Larry</h4>
            <p>1 year old, tabby</p>
            <p>Playful, friendly</p>
        </div>
        <!-- Adoption Info -->
        <div class="adoption-info">
            <span class="tag">Adoption</span>
            <h3>Find your perfect Companion</h3>
            <p>
                Give a loving home to pets in need. Our adoption process ensures
                the perfect match between pets and their new families.
            </p>
            <a href="#" class="view-pets-btn">View available pets ‚Üí</a>
        </div>
    </div>
</section>
<!-- Rescue Section -->
<section class="rescue-section">
    <div class="rescue-container">
        <div class="rescue-info">
            <span class="tag">Rescue</span>
            <h3>Pet Rescue </h3>
            <p>
                We are dedicated to rescuing abandoned and injured pets.
                Our team provides shelter, food, and medical care to help pets recover
                and find their forever homes.
            </p>
            <a href="@Url.Action("Report", "Rescue")" class="browse-btn">Report ‚Üí</a>
        </div>
        <div class="rescue-grid">
            <div class="rescue-card">
                <img src="~/Content/image/care.png" alt="Care" />
                <p>Care</p>
            </div>
            <div class="rescue-card">
                <img src="~/Content/image/doctor.png" alt="Vet" />
                <p>Vet</p>
            </div>
            <div class="rescue-card">
                <img src="~/Content/image/house.png" alt="Shelter" />
                <p>Shelter</p>
            </div>
            <div class="rescue-card">
                <img src="~/Content/image/adoption.png" alt="Adopt" />
                <p>Adopt</p>
            </div>
        </div>
    </div>
</section>

<!-- Pet Diary Section -->
<section class="diary-section">
    <div class="diary-container">
        <!-- Left Column -->
        <div class="diary-info">
            <span class="tag">Pet Diary</span>
            <h3>Keep a Journal of Your Cat‚Äôs Life</h3>
            <p>
                Record special memories, track health updates, and note vaccination dates for your beloved pet.
                Keep all the important moments and milestones in one place.
            </p>
            <a href="#" class="browse-btn">Explore Pet Diary ‚Üí</a>
        </div>
        <!-- Right Column -->
        <div class="diary-texts">
            <ul>
                <li>Save your cat‚Äôs cutest moments.</li>
                <li>Keep an eye on health updates.</li>
                <li>Note birthdays, milestones, and visits.</li>
                <li>Write about daily activities or special moments.</li>
            </ul>
        </div>
    </div>
</section>


@Html.Partial("_Footer")


